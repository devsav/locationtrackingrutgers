<h1>New user</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :loginName %><br />
    <%= f.text_field :loginName %>
  </p>
  <p>
    <%= f.label :hashed_password %><br />
    <%= f.text_field :hashed_password %>
  </p>
  <p>
    <%= f.label :salt %><br />
    <%= f.text_field :salt %>
  </p>
  <p>
    <%= f.label :userName %><br />
    <%= f.text_field :userName %>
  </p>
  <p>
    <%= f.label :contactNumber %><br />
    <%= f.text_field :contactNumber %>
  </p>
  <p>
    <%= f.label :address %><br />
    <%= f.text_area :address %>
  </p>
  <p>
    <%= f.label :emailId %><br />
    <%= f.text_field :emailId %>
  </p>
  <p>
    <%= f.label :registered %><br />
    <%= f.check_box :registered %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', users_path %>
